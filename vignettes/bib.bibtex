@article{Murphy1983,
abstract = {The dynamics of a population depend upon the sizes and/or maturity of the individual members of the population. In many sepcies (the most popular examples are fish and trees) the growth and maturation rate of an individual is controlled by the ability of that individual to abtain necessary resources. For such a species, individual growth rates depend upon the overall condition of the population. The purpose of this paper is to develop a continous model with size structure, in which individual growth rates are not known a priori. An individual's size depends not only upon it's age, but upon his history of the general population during its past life. The resulting model is a nonlinear variation of the classical McKendrick-von Foerster model. The mathamtical descriptions of growth processes which originate in the biological literature can be used as constitutive equations in our model. In some cases, such substitutions allow the model to be reduced to a system of ordinary differential equations.},
author = {Murphy, L.F.},
file = {::},
journal = {Journal of theoretical biology},
pages = {493--506},
publisher = {Elsevier},
title = {{A nonlinear growth mechanism in size structured population dynamics}},
url = {http://www.sciencedirect.com/science/article/pii/0022519383902424},
volume = {104},
year = {1983}
}
