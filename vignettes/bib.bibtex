journal = {Fisheries Research},
keywords = {Biomass,Mortality,Production model,Yield},
pages = {69--79},
title = {{Sustainable yield indicators from biomass: Are there appropriate reference points for use in tropical fisheries?}},
volume = {32},
year = {1997}
}
@article{Chrysafi2015,
abstract = {Estimation of population abundances in the absence of good observational data is notoriously difficult, yet urgently needed for biodiversity conservation and sustainable use of natural resources. In the field of fisheries research, management regulations have for long demanded population abundance estimates even if data available are sparse, leading the development of a range of fish stock assessment methods designed for data-poor populations. Here, we present methods developed within the context of fisheries research that can be applied to conduct population abundance estimations when facing data-limitations. We begin the review from the less data-demanding approaches and continue with more data-intensive ones. We discuss the advantages and caveats of these approaches the challenges and management implications associated with data-poor stock assessments and we propose the implementation of the Bayesian hierarchical framework as the most promising avenue for future development and improvement of the current practices},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Chrysafi, Anna and Kuparinen, Anna},
doi = {10.1017/CBO9781107415324.004},
eprint = {arXiv:1011.1669v3},
file = {:home/alko/Documents/Mendeley Desktop/Chrysafi, Kuparinen - 2015 - Assessing abundance of populations with limited data Lessons learned from data-poor fisheries stock assessm.pdf:pdf},
isbn = {9788578110796},
issn = {1098-6596},
journal = {Environmental Reviews},
keywords = {icle},
number = {October 2015},
pages = {1--44},
