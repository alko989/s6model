@article{White2007,
abstract = {Body size is perhaps the most fundamental property of an organism and is related to many biological traits, including abundance. The relationship between abundance and body size has been extensively studied in an attempt to quantify the form of the relationship and to understand the processes that generate it. However, progress has been impeded by the under appreciated fact that there are four distinct, but interrelated, relationships between size and abundance that are often confused in the literature. Here, we review and distinguish between these four patterns, and discuss the linkages between them. We argue that a synthetic understanding of size-abundance relationships will result from more detailed analyses of individual patterns and from careful consideration of how and why the patterns are related.},
author = {White, Ethan P and Ernest, S K Morgan and Kerkhoff, Andrew J and Enquist, Brian J},
doi = {10.1016/j.tree.2007.03.007},
file = {:home/alko/Documents/Mendeley Desktop//White et al. - 2007 - Relationships between body size and abundance in ecology.pdf:pdf},
issn = {0169-5347},
journal = {Trends in ecology {\&} evolution},
keywords = {Animals,Body Size,Ecology,Ecosystem,Models, Biological,Population Density},
month = {jun},
number = {6},
pages = {323--30},
pmid = {17399851},
title = {{Relationships between body size and abundance in ecology.}},
