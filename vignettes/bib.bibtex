@article{Coutant1987,
abstract = {Most fish species (and sometimes different life stages within a species) exhibit behavioral temperature selection. In nature, thermal gradients provide habitat structure to which fish orient. Apparently, fish have evolved to fit into 'thermal niches' to optimize physiological, ecological, and reproductive performance in their native habitats. However, man has perturbed many of these habitats to the point where previously advantageous thermal preference behaviour may now be inapropriate. Termal perturbations are the most obviously related examples of man's interference, but fish's inherent thermal preference can magnify the biologcial effects of changes in dissolved oxygen, food supply, toxicants and physical barriers. Environmental management should (1) consider thermal preference behavior when evaluating habitat (2) manage for needed temperature and thermal structure, and (3) give priority attention minimizing degradation of preferred thermal habitat, especially when it is in short supply.},
author = {Coutant, Charles C},
file = {::},
journal = {Environmental biology of fishes},
keywords = {temperature},
number = {3},
pages = {161--172},
title = {{Thermal preference: when does an asset become a liability?}},
url = {http://www.springerlink.com/index/K215UN6130544R42.pdf},
volume = {18},
year = {1987}
}
