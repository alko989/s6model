        </m:b>
      },
author = {Beyer, JE E},
file = {::},
journal = {Dana},
pages = {45--147},
title = {{Recruitment stability and survival - simple size-specific theory with examples from the early life dynamics of marine fish}},
url = {http://scholar.google.com/scholar?hl=en{\&}btnG=Search{\&}q=intitle:Recruitment+stability+and+survival+simple+size-specific+theory+with+examples+from+the+early+life+dynamics+of+marine+fish{\#}0 http://scholar.google.com/scholar?hl=en{\&}btnG=Search{\&}q=intitle:Recruitment+stability+and+survival-simple+size-specific+theory+with+examples+from+the+early+life+dynamics+of+marine+fish{\#}0},
