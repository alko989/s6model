month = {jan},
number = {1},
pages = {1--15},
pmid = {18047588},
title = {{Demographic analysis of continuous-time life-history models.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2228373{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {11},
