@article{Schnute1987,
abstract = {This paper describes a fishery model based on sine, rather than age, as ane fundamental population variable. The model's key ingredient is a density function related to numbel. or fish within any given size range. A key assumption is that selectivity in the fishery is determined by size, not age, and that fish can reach recruitment sine at various ages. The theory easily yields moment equations comparable with those obtained recently by Fournier and Doonan; far example, the general moment equation here is derived in a four-line proof. The paper gives detailed reeornrnendations for tailoring the new model to a specific fishery, where model complexity is deter- mined by the available data. The new theory places earlier models of Bleriso and Schnute in the context of generalized cohort analysis, where size-dependent mortality is allowed In particular, the paper identifies a potential problem with these earlier models and proposes a solution. All results are interpreted biologically to provide both conceptual and analytical tools far stock assessment.},
author = {Schnute, Jon},
file = {::},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {5},
pages = {924--940},
publisher = {NRC Research Press},
title = {{A general fishery model for a size-structured fish population}},
url = {http://www.nrcresearchpress.com/doi/abs/10.1139/f87-111},
volume = {44},
year = {1987}
}
