primitiv, men egentlig ganske st�rk.},
author = {Pope, John G and Rice, Jake C and Daan, Niels and Jennings, Simon and Gislason, Henrik},
doi = {10.1016/j.icesjms.2006.04.015},
file = {::;::},
journal = {ICES Journal of Marine Science},
keywords = {accepted 8 april 2006,compensation,ecosystem approach to fisheries,exploitation,fish community,g,j,n-9000 troms{\o},norwegian college of fishery,pope,predation mortality,received 25 august 2005,science,size spectrum,stock-recruitment relationship,university of troms{\o}},
